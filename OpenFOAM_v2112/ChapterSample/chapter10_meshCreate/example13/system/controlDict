FoamFile
{
    version 2.0;
    format ascii;
    class dictionary;
    location system;
    object controlDict;
}
    application simpleFoam;
    startFrom startTime;
    startTime 0;
    stopAt endTime;
    endTime 3000;
    deltaT 1;
    writeControl timeStep;
    writeInterval 1000;
    purgeWrite 0;
    writeFormat ascii;
    writePrecision 10;
    writeCompression uncompressed;
    timeFormat general;
    timePrecision 6;
    graphFormat raw;
    runTimeModifiable yes;
